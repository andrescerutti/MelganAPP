<%= form_for @publication do |f| %>
    <%= f.label :categoria, "ingrese una categoria" %>
    <%= f.text_field :categoria %>
    <%= f.label :cantidad, "ingrese una cantidad de animales" %>
    <%= f.text_field :cantidad %>
    <%= f.label :raza, "indique la raza de los animales" %>
    <%= f.text_field :raza %>
    <%= f.label :oferta, "1 si es oferta" %>
    <%= f.text_field :oferta %>
    <%= f.label :demanda, "1 si es oferta" %>
    <%= f.text_field :demanda %>
    <%= f.submit %>
<% end %>
